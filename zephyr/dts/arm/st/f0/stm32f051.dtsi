#include <st/f0/stm32f0.dtsi>

/ {
	soc {
		dac1: dac@40007400 {
			compatible = "st,stm32-dac";
			reg = <0x40007400 0x400>;
			clocks = <&rcc STM32_CLOCK_BUS_APB1 0x20000000>;
			status = "disabled";
			label = "DAC_1";
			#io-channel-cells = <1>;
		};

		flash-controller@40022000 {
			flash0: flash@8000000 {
				erase-block-size = <1024>;
			};
		};
	};
};
