config GPIO_GECKO
	default y
	depends on GPIO || LOG_BACKEND_SWO

config UART_GECKO
	default y
	depends on SERIAL

config I2C_GECKO
	default y
	depends on I2C

config SOC_FLASH_GECKO
	default y
	depends on FLASH

config SPI_GECKO
	default y
	depends on SPI
