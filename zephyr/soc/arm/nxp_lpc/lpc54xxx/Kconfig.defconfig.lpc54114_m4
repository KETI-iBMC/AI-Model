if SOC_LPC54114_M4

config SOC
	default "lpc54114"

config PINMUX_MCUX_LPC
	default y
	depends on PINMUX

config GPIO_MCUX_LPC
	default y
	depends on GPIO

config UART_MCUX_FLEXCOMM
	default y
	depends on SERIAL

config SOC_FLASH_LPC
	default y
	depends on FLASH

endif # SOC_LPC54114_M4
