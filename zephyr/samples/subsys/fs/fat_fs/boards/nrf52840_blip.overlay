&spi1 {
        status = "okay";
        cs-gpios = <&gpio0 17 GPIO_ACTIVE_HIGH>;

        sdhc0: sdhc@0 {
                compatible = "zephyr,mmc-spi-slot";
                reg = <0>;
                status = "okay";
                label = "SDHC0";
                spi-max-frequency = <24000000>;
        };
};
