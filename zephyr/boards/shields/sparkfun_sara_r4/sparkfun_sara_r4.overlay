&arduino_serial {
	current-speed = <115200>;
	status = "okay";

	sara_r4 {
		compatible = "ublox,sara-r4";
		label = "ublox-sara-r4";
		mdm-power-gpios = <&arduino_header 11 0>; /* D5 */
		mdm-reset-gpios = <&arduino_header 12 0>; /* D6 */
		status = "okay";
	};
};
