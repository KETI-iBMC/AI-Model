&arduino_i2c {

	hts221@5f {
		compatible = "st,hts221";
		reg = <0x5f>;
		label = "HTS221";
	};

	lps25hb-press@5d {
		compatible = "st,lps25hb-press";
		reg = <0x5d>;
		label = "LPS25HB";
	};

	lis3mdl-magn@1e {
		compatible = "st,lis3mdl-magn";
		reg = <0x1e>;
		irq-gpios = <&arduino_header 5 GPIO_ACTIVE_HIGH>; /* DRDY on A5 */
		label = "LIS3MDL";
	};

	lsm6ds0@6b {
		compatible = "st,lsm6ds0";
		reg = <0x6b>;
		label = "LSM6DS0";
	};
};
