&arduino_spi {
	status = "okay";
	cs-gpios = <&arduino_header 16 GPIO_ACTIVE_LOW>;	/* D10 */

	enc424j600@0 {
		compatible = "microchip,enc424j600";
		spi-max-frequency = <14000000>;
		int-gpios = <&arduino_header 15 GPIO_ACTIVE_LOW>;	/* D9 */
		status = "okay";
		label = "ETH_0";
		reg = <0>;
	};
};
