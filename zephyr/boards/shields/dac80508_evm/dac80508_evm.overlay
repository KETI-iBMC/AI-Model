&arduino_spi {
	status = "okay";
	cs-gpios = <&arduino_header 16 GPIO_ACTIVE_LOW>;	/* D10 */

	dac80508: dac80508@0 {
		compatible = "ti,dac80508";
		reg = <0x0>;
		label = "DAC80508";
		spi-max-frequency = <1000000>;
		#io-channel-cells = <1>;
	};
};
