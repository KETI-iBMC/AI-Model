	prompt "Board Firmware Version"
	default BOARD_EM_STARTERKIT_R23
	depends on BOARD_EM_STARTERKIT

	config BOARD_EM_STARTERKIT_R22
		bool "2.2"

	config BOARD_EM_STARTERKIT_R23
		bool "2.3"

endchoice
